
select count (c.CourseSessionTypeid) as TotalSectionENGL

    
    from 
edw.Course c
left join edw.Subject s on c.Subjectid=s.Subjectid
left join edw.Term t on c.Termid=t.Termid
left join edw.CourseStatus cs on c.Courseid=cs.CourseStatusid

where t.TermName='Fall 2022'
and  s.SubjectCode='ENGL'
and s.remove=0
