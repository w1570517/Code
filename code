SELECT d.DegreeAbbrDesc FROM 
rm_Enrollment.StudentMajorEnrolled_C_F e 
LEFT JOIN  edw.Term t   ON e.TermId=t.TermId
LEFT JOIN edw.DegreeFlat d on e.DegreeCode=d.DegreeAbbreviationLevelCode
left join edw.TermType tp on t.TermTypeId=tp.TermTypeId
left join rm_Dimension_dim_TermLast5Years dt on tp.TermTypeId=dt.TermTypeId
WHERE 
e.Degreecode='BS' AND e.department='nursing'
AND  
dt.FiscalYear IN ('2018','2019','2020','2021','2022') 

AND tp.TermType='Fall'
