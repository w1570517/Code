 
SELECT e.department, dt.FiscalYear as Year ,count(e.StudentId) as total_enrollment__BS_Nursing_Last5years

From 
rm_Enrollment.StudentMajorEnrolled_C_F e 

left join rm_Dimension_dim_TermLast5Years dt ON e.Termid=dt.Termid
left join edw.TermType tp ON t.TermTypeId=tp.TermTypeId

WHERE 
e.Degreecode='BS' AND e.department='nursing'
AND  
dt.FiscalYear IN ('2018','2019','2020','2021','2022') 

AND tp.TermType='Fall'


Group by e.department,dt.FiscalYear

order by total_enrollment__BS_Nursing_Last5years desc
