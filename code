
--questions 2 full query 


select count (c.CourseSessionTypeid) as TotalSectionENGLSenior

    
    from 
edw.Course c
left join edw.Subject s on c.Subjectid=s.Subjectid
left join edw.Term t on c.Termid=t.Termid
left join edw.CourseStatus cs on c.Courseid=cs.CourseStatusid
left join edw.Campus cp on c.Campusid=cp.Campusid

where t.TermName='Fall 2022'
and  s.SubjectCode='ENG'
and c.remove=0
and cs.CourseStatus='Offered'
