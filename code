
SELECT Gender, Age, Ethnicity,

CASE WHEN NET = 1 COUNT (StudentID) END AS 'NETStudentCount',
CASE WHEN NEF = 1 COUNT (StudentID) END AS 'NEFStudentCount' 
FROM
     rm_Retention.StudentRetentionDegree 
WHERE
   CohortTermName = 'Fall 2022'

GROUP BY Gender, Age, Ethnicity

Order by NETStudentCount Desc, NEFStudentCount Desc




